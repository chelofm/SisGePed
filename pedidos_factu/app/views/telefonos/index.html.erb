<h1>Listing telefonos</h1>

<table>
  <tr>
    <th>Domicilio</th>
    <th>Trabajo</th>
    <th>Movil1</th>
    <th>Movil2</th>
  </tr>

<% @telefonos.each do |telefono| %>
  <tr>
    <td><%=h telefono.domicilio %></td>
    <td><%=h telefono.trabajo %></td>
    <td><%=h telefono.movil1 %></td>
    <td><%=h telefono.movil2 %></td>
    <td><%= link_to 'Show', telefono %></td>
    <td><%= link_to 'Edit', edit_telefono_path(telefono) %></td>
    <td><%= link_to 'Destroy', telefono, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New telefono', new_telefono_path %>