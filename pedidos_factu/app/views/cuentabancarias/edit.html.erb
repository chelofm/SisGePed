  <%= f.error_messages %>

  <p>
    <%= f.label :numerocuenta %><br />
    <%= f.text_field :numerocuenta %>
  </p>
  <p>
    <%= f.label :proveedor_id %><br />
    <%= f.text_field :proveedor_id %>
  </p>
  <p>
    <%= f.label :tipocuenta_id %><br />
